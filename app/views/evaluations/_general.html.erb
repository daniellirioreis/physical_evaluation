<div class="row-fluid"> 
 <div class="span12">
   <%= f.input :student_id, as: :hidden %>
   <%= f.input :student_name, :url => autocomplete_student_name_students_path, :as => :autocomplete , :id_element => '#evaluation_student_id', :placeholder => "Digite o nome do aluno", :multiple => false, input_html: {class:'span7'} %>
 </div>
</div>   
<div class="row-fluid"> 
   <div class="span12"> 
      <%= f.input :evaluator_id, as: :hidden %>
      <%= f.input :evaluator_name, :url => autocomplete_evaluator_name_evaluators_path, :as => :autocomplete , :id_element => '#evaluation_evaluator_id', :placeholder => "Digite o nome do Avaliador", :multiple => false, input_html: {class:'span7'} %>
   </div>
</div>
<div class="row-fluid">    
   <div class="span4"> 
      <%= f.input :evaluation_date, input_html: { class: 'span4' } %>
   </div>
</div>
<div class="row-fluid">   
   <div class="span4"> 
      <%= f.input :end_date, input_html: { class: 'span4' } %>
   </div>
</div>   
