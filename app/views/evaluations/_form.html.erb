<%= simple_form_for(@evaluation) do |f| %>
  <div class="tabbable">
    <ul class="nav nav-tabs">
      <li class="active"> <%= link_to "Geral", "#general", :"data-toggle" => :tab %></li>
      <li> <%= link_to "Anamanese", "#anamnese", :"data-toggle" => :tab %></li>
    </ul>
  </div>
  <div class="tab-content">
    <div class="tab-pane fade active in" id="general">
      <%= render "general", :f => f %>
    </div>
    <div class="tab-pane" id="anamnese">
      <%= render "anamnese", :f => f %>
    </div>
  </div>
  <div class="form-actions">
    <%= f.button :submit, :class => "btn btn-success" %>
    <%= link_to 'Voltar', students_path, :class => "btn"  %>
  </div>  
<% end %>
